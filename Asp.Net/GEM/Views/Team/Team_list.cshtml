@model  List<GEM.Models.ModelTeam>

@{
    ViewBag.Title = "Create Team";
}

<script src="~/Scripts/Team/Team.js" type="text/jscript"></script>
<section id="CreateTeam" class="col-lg-12">
    <div class="container">

        <div class="col-lg-12 dashboardcontent">

            <div class="col-lg-3 left-mainlinks">

                <ul class="main-links-left">
                    <a href=""> <li class="active"> <h3>Manage Teams </h3> <img src="images/team-menu-icon.png" alt="teams"> </li> </a>
                </ul>

            </div>

            <div class="col-lg-6 center-secondarylinks">

                <div class="col-lg-12 topbararea">
                    <img src="images/team-icon.png" alr="J1"> <h4 class="journeytitle"> Teams </h4>
                </div>

                <div class="row">

                    <div class="col-lg-12 list-teams" id="List">
                        <ul>
                            @if (Model.Count > 0)
                            {
                                foreach (var team_list in Model)
                                {
                                    var imageName = "../images/rocket.png";
                                    <a href="../Team/Create?Journey=@team_list.teamId"><li class="active"> <img src=@imageName> @team_list.teamName </li> </a>
                                }
                            }
                        </ul>

                    </div>
                </div>

            </div>

            <div class="col-lg-3 right-links">

                <ul class="main-links-right">
                    <a href=""> <li> Create Team <img src="~/images/right-link-icon.png" alt="#"> </li> </a>
                    <a href=""> <li> Edit Team <img src="~/images/right-link-icon.png" alt="#"> </li> </a>
                    <a href=""> <li> Select Journey <img src="~/images/right-link-icon.png" alt="#"> </li> </a>
                    <a href=""> <li> View Practices <img src="~/images/right-link-icon.png" alt="#"> </li> </a>
                    <a href=""> <li> View Results <img src="~/images/right-link-icon.png" alt="#"> </li> </a>
                </ul>

            </div>

        </div>

    </div>
</section>

<script>
    jQuery(document).ready(function () {

        jQuery('.navbar-toggle').click(function () {
            jQuery('.navbar-right').toggle();
        });
    });
</script>

